#include <stdio.h>

int main() {
    int n, i, fatorial = 1; 

    scanf("%d", &n);

    if (n < 0) {
        printf("Não existe fatorial de números negativos\n");
    } else {
        for (i = 1; i <= n; i++) {
            fatorial *= i;
        }
        printf("O fatorial de %d é %d\n", n, fatorial);
    }

    return 0;
}
